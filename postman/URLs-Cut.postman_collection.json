									"    console.log('Nenhum URL ID encontrado. Criando uma URL primeiro...');",
									"    ",
									"    pm.sendRequest({",
									"        url: pm.environment.get('base_url') + '/api/urls',",
									"        method: 'POST',",
									"        header: {",
									"            'Content-Type': 'application/json',",
									"            'Authorization': 'Bearer ' + pm.environment.get('access_token')",
									"        },",
									"        body: {",
									"            mode: 'raw',",
									"            raw: JSON.stringify({",
									"                originalUrl: 'https://www.example.com/temp'",
									"            })",
									"        }",
									"    }, function (err, res) {",
									"        if (!err && res.code === 201) {",
									"            var jsonData = res.json();",
									"            if (jsonData.id) {",
									"                pm.environment.set('last_url_id', jsonData.id);",
									"            }",
									"        }",
									"    });",
									"}"
								],
								"type": "text/javascript"
							}
						},
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 204\", function () {",
									"    pm.response.to.have.status(204);",
									"});"
								],
								"type": "text/javascript"
							}
